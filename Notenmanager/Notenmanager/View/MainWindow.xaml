<Window x:Class="Notenmanager.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notenmanager.View"
        mc:Ignorable="d"
        Title="Notenmanager" Height="800" Width="600"
        MinHeight="800"
        MinWidth="600"
        SizeToContent="WidthAndHeight"
        DataContext="{StaticResource MainPageVM}">
    <Grid>
        <Frame x:Name="frMainFrame" Source="MainPage.xaml" HorizontalAlignment="Stretch" Height="Auto" NavigationUIVisibility="Visible" VerticalAlignment="Stretch" Width="Auto" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"/>
    </Grid>
</Window>
